{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@core/*": [
        "../packages/core/docs/*"
      ],
      "@css/*": [
        "../packages/css/docs/*"
      ],
      "@dom/*": [
        "../packages/dom/docs/*"
      ],
      "@resource/*": [
        "../packages/resource/docs/*"
      ],
      "@router/*": [
        "../packages/router/docs/*"
      ],
      "@store/*": [
        "../packages/store/docs/*"
      ]
    }
  }
}