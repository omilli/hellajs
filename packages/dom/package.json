{
  "name": "@hellajs/dom",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git@github.com:omilli/hellajs.git",
    "directory": "./"
  },
  "homepage": "git+https://github.com/omilli/hellajs.git",
  "bugs": {
    "url": "https://github.com/omilli/hellajs/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/hellajs-dom.cjs.js",
  "module": "./dist/hella-dom.esm.js",
  "unpkg": "./dist/hella-dom.esm.min.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/hella-dom.esm.js",
      "require": "./dist/hella-dom.cjs.js"
    },
    "./esm/*": {
      "import": "./dist/esm/*.js"
    },
    "./esm-min/*": {
      "import": "./dist/esm-min/*.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "peerDependencies": {
    "@hellajs/core": "^0.8.0"
  },
  "version": "0.8.6"
}